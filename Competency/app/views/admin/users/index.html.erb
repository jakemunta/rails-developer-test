<div class="container">
  <div class="col-sm-10 col-sm-offset-1 col-xs-12 blog-content">
    <% @users.each do |user| %>
    <div class="col-xs-12">
      <div class="text-center">
        <h2><%= user.email %></h2>
        <h6><%= user.created_at.strftime('%b %d, %Y') %></h6>

      <div class="text-center">
        <%= link_to "READ MORE", admin_user_path(user) %>
      </div>
      <br>
<div class="text-center">
        <%= link_to "Edit", edit_admin_user_path(user) if current_user && current_user.admin? %>
      </div>
      <br>
    </div>
    <% end %>

   <p><% if current_user && (current_user.admin?)%><%= link_to "New User", new_admin_user_path%><% end%></p>


  </div>
</div>

